<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:Ai_Organizer.ViewModels"
             x:DataType="vm:PlanPreviewViewModel"
             x:Class="Ai_Organizer.Views.PlanPreviewView">
    <StackPanel Margin="8" Spacing="10">
        <TextBlock Classes="h2" Text="Plan Preview" />
        <Border Padding="12" CornerRadius="10" BorderThickness="1" BorderBrush="{DynamicResource SystemControlForegroundBaseLowBrush}">
            <TextBlock Classes="muted" Text="{Binding Placeholder}" TextWrapping="Wrap" />
        </Border>
    </StackPanel>
</UserControl>


